.body-wrap.boxed-container
  header.site-header
    .header-sun.is-revealing
      svg(src='/icon/wcu.svg')
    .container
      .site-header-inner
        .brand.header-brand
          h1.m-0
            a(href='/landing')
              svg()
  main
    section.hero
      .container
        .hero-inner
          .hero-copy
            h1.hero-title.mt-0 Wild Chicken Univeristy
            p.hero-paragraph
              | A place to learn.
            p.hero-cta
              a.button.button-primary.button-shadow(href='users/register') Get Started
              a(href='/users/login') 
                p.hero-paragraph Returning users?
          .hero-illustration
            .hero-shapes.is-revealing
              img(src='/icon/wcu.svg')
    section.clients.section
      .container
        .features-extended-inner.section-inner.has-top-divider
          .features-extended-header.text-center
            .container-sm
              h2.section-title.mt-0#about Start Learning Now:
              p.section-paragraph
                | 
        .hero-form.newsletter-form.field.field-grouped.is-revealing
          .control.control-expanded
            input.input(type='text' name='class' placeholder='Ex: Linear Algebra')
          .control 
            input.button.button-primary.button-block.button-shadow(type='submit', name='submit', value='Search')
        p.section-paragraph
          | 
